{
  "liveSassCompile.settings.formats": [
    {
      "format": "expanded",
      "extensionName": ".css",
      "savePath": "/folder1/static/css"
    },
    {
      "format": "expanded",
      "extensionName": ".css",
      "savePath": "/folder2/static/css"
    }
  ],
  "liveSassCompile.settings.includeItems": [
    "folder1/src/scss/**/*.scss",
    "folder2/src/scss/**/*.scss"
